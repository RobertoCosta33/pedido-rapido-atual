{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft": "Warning",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "PedidoRapido": "Information",
      "System": "Warning"
    },
    "Console": {
      "IncludeScopes": false
    }
  },
  "AllowedHosts": "*",
  "UseEntityFramework": true,
  "ConnectionStrings": {
    "DefaultConnection": "PLACEHOLDER_WILL_BE_OVERRIDDEN_BY_ENV"
  },
  "JwtSettings": {
    "Secret": "PLACEHOLDER_WILL_BE_OVERRIDDEN_BY_ENV",
    "Issuer": "PedidoRapido.API",
    "Audience": "PedidoRapido.Frontend",
    "ExpirationInHours": 24
  },
  "Stripe": {
    "SecretKey": "PLACEHOLDER_WILL_BE_OVERRIDDEN_BY_ENV",
    "PublishableKey": "PLACEHOLDER_WILL_BE_OVERRIDDEN_BY_ENV",
    "WebhookSecret": "PLACEHOLDER_WILL_BE_OVERRIDDEN_BY_ENV",
    "PriceIds": {
      "Basic": {
        "Monthly": "price_basic_monthly_prod",
        "Semiannual": "price_basic_semiannual_prod",
        "Annual": "price_basic_annual_prod"
      },
      "Pro": {
        "Monthly": "price_pro_monthly_prod",
        "Semiannual": "price_pro_semiannual_prod",
        "Annual": "price_pro_annual_prod"
      },
      "Premium": {
        "Monthly": "price_premium_monthly_prod",
        "Annual": "price_premium_annual_prod"
      }
    }
  },
  "Cors": {
    "AllowedOrigins": [
      "https://pedidorapido.vercel.app",
      "https://www.pedidorapido.com",
      "https://pedidorapido.com"
    ]
  }
}
